{
  "name": "ffs-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node-dev --inspect --respawn --transpileOnly src/index.ts",
    "migrate:make": "knex --knexfile src/knexfile.ts -x ts migrate:make",
    "migrate": "knex --knexfile src/knexfile.ts migrate:latest",
    "migrate:test": "cross-env NODE_ENV=test knex --knexfile src/knexfile.ts migrate:latest",
    "migrate:rollback": "knex --knexfile src/knexfile.ts migrate:rollback",
    "seed:make": "knex --knexfile src/knexfile.ts -x ts seed:make",
    "seed": "knex --knexfile src/knexfile.ts seed:run",
    "seed:test": "cross-env NODE_ENV=test knex --knexfile src/knexfile.ts seed:run",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "cross-env NODE_ENV=test jest"
  },
  "keywords": ["node", "express", "typescript", "objection", "knex", "joi", "jest"],
  "author": "Aleksandr Drobot",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^4.0.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http-errors": "latest",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.10",
    "objection": "^2.1.3",
    "objection-visibility": "^1.0.0",
    "passport": "^0.4.1",
    "pg": "^7.18.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.7.5",
    "uuid": "^7.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/http-errors": "^1.6.3",
    "@types/jest": "^25.1.3",
    "@types/joi": "^14.3.4",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.7.2",
    "@types/passport": "^1.0.2",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^7.0.0",
    "cross-env": "^7.0.1",
    "dotenv": "^8.2.0",
    "jest": "^25.1.0",
    "morgan": "^1.9.1",
    "supertest": "^4.0.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^6.0.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0"
  }
}
